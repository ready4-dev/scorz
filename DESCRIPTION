Package: scorz
Title: Score Multi-Attribute Utility Instruments
Version: 0.0.0.9067
Authors@R: c(
    person("Matthew", "Hamilton", , "matthew.hamilton1@monash.edu", role = c("aut", "cre", "cph"),
           comment = c(ORCID = "0000-0001-7407-9194")),
    person("Caroline", "Gao", , "caroline.gao@orygen.org.au", role = "aut",
